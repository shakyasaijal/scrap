    "\"\"\"\n",
    "INPUTS:\n",
    "    text: a string\n",
    "OUTPUTS: \n",
    "    a better formatted string\n",
    "\"\"\"\n",
    "def unicode_normalize(text):\n",
    "    return text.translate({ 0x2018:0x27, 0x2019:0x27, 0x201C:0x22, 0x201D:0x22,\n",
    "                            0xa0:0x20 }).encode('utf-8')"
   ]
  },
